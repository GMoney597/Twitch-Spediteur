<Window x:Class="Twitch_Spediteur.Fenster.Entfernungen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Twitch_Spediteur.Fenster"
        mc:Ignorable="d"
        Title="Entfernungen" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="5*" />
        </Grid.ColumnDefinitions>
        <Button Name="cmdImport" Content="Importieren" Grid.Column="0" VerticalAlignment="Center" Margin="10" Click="cmdImport_Click"/>
        <DataGrid Name="dtgEntfernungen" Grid.Column="1" Margin="10" AutoGenerateColumns="True">
            
        </DataGrid>
    </Grid>
</Window>
